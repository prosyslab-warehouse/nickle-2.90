autoimport PRNG;

int[*] t(int n) {
  int[2] s = {0, 0};
  int i;
  for (i = 0; i < n; i++)
    s[randint(2)]++;
  return s;
}

File::printf ("%d %d\n", t (string_to_integer (argv[1]))...);
